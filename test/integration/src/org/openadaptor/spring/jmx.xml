<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
  
	<bean id="MBeanServer" class="org.openadaptor.core.jmx.MBeanServer">
    <constructor-arg value="8082"/>
  </bean>
  
  <bean id="SystemUtil" class="org.openadaptor.util.SystemUtil"/>
  
  <bean id="Test" class="org.openadaptor.spring.JmxTestCase$Test">
    <constructor-arg value="hello world"/>
	</bean>
  
  <bean id="dummy" class="java.lang.String">
    <constructor-arg value="test"/>
  </bean>
  
</beans>